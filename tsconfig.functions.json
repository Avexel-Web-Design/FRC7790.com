{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "exactOptionalPropertyTypes": false,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["functions/**/*.ts"]
}
